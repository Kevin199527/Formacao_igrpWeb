;\n    padding: 20px;\n    border-radius: 8px;\n    background-color: #ffffff;\n    box-shadow: 0 4px 8px rgba(255, 0, 0, 0.2), 0 0 15px rgba(0, 128, 0, 0.1), 0 20px 20px -15px rgba(0, 0, 255, 0.2);\n    font-family: Arial, sans-serif;\n}\n\n.tabela th {\n    border-radius: 8px;\n    background-color: #009879;\n    color: #ffffff;\n    text-align: left;\n    padding: 12px 15px;\n}\n\n\n/* Estilo das células de dados */\n.tabela td {\n    padding: 12px 15px;\n}\n\n/* Estilo para linhas alternativas */\n.tabela tr:nth-child(even) {\n    background-color: #f3f3f3;\n}\n\n/* Estilo para a classe nometabela */\n.nome_tabela {\n    font-weight: bold;\n    color: #009879;\n}\n\n/* Estilo para a classe contactabela */\n.contac_tabela {\n    font-style: italic;\n    color: #333333;\n}\n\n/* Estilo para a classe numerotabela */\n.numero_tabela {\n    text-align: right;\n    color: #555555;\n}","js":"","files":{"css":[],"js":[],"xsl":[]},"blocklyData":"<xml><block type=\"controller\" id=\"controller\" x=\"388\" y=\"13\"></block></xml>"}
